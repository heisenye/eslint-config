    "typescript": "^5.8.3",
    "unbuild": "^2.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@typescript-eslint/parser": "^8.36.0",
    "@typescript-eslint/utils": "^8.36.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.36.0"
