{
  "name": "@heisenye/eslint-config",
  "version": "0.3.1",
  "description": "heisenye's eslint config",
  "keywords": [
    "eslint-config"
  ],
  "author": "heisenye <heisenye@163.com> (https://github.com/heisenye/)",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "type": "module",
  "main": "dist/index.mjs",
