{
  "name": "@heisenye/eslint-config",
  "version": "0.3.1",
  "description": "heisenye's eslint config",
  "keywords": [
    "eslint-config"
  ],
  "author": "heisenye <heisenye@163.com> (https://github.com/heisenye/)",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "type": "module",
  "main": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "test": "pnpm --filter ./test/ run test",
    "lint": "eslint . --cache"
  },
  "devDependencies": {
    "@heisenye/prettier-config": "^0.0.1",
    "@types/node": "^22.16.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@typescript-eslint/utils": "^8.35.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.35.1"
  }
}
