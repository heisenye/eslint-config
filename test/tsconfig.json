{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* Target */
    "jsx": "preserve",
    "jsxImportSource": "vue",
    /* Module */
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "../src/*"
      ]
    },
    /* Linting */
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "noUncheckedSideEffectImports": false
  }
}
