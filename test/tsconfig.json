{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* Target */
    "jsx": "preserve",
    "jsxImportSource": "vue",

    /* Linting */
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "noUncheckedSideEffectImports": false
  }
}
